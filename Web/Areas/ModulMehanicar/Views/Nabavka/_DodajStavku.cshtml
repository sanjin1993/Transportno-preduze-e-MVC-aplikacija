@model Web.Areas.ModulMehanicar.Models.StavkaNabavkaVM
@using (Ajax.BeginForm("SnimiStavku", new AjaxOptions { HttpMethod = "get", OnSuccess = "updateModel(data);", UpdateTargetId = "stavkeList", InsertionMode = InsertionMode.Replace }))
{
    <div class="x_panel"style="width:300px; height:360px">
   
        <div class="x_title">
            <h3 class="x-title">Nova stavka</h3>
        </div>
        <div class="panel-body detalji">
            <div class="form-group">
                <label>Naziv stavke</label>
                @Html.TextBoxFor(model => model.Naziv, new { @class = "form-control" })
            </div>
            <div class="form-group">
                <label>Cijena </label>
                @Html.TextBoxFor(model => model.Cijena, new { @class = "form-control" })
            </div>
         


            @Html.HiddenFor(model => model.NabavkaId)
            <input type="submit" value="Dodaj stavku" class="btn btn-success input_16" />
        </div>
    </div>
}
